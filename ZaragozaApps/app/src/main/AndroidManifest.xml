<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zaragozaapps.zaragozaapps" >

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.zaragozaapps.touchgame.TouchGame"
            android:label="@string/touch_game"
            android:parentActivityName="com.example.zaragozaapps.zaragozaapps.MainActivity" >
            <meta-data
                android:name="Touch Game"
                android:value="com.example.zaragozaapps.touchgame.TouchGame" />
        </activity>

        <activity
            android:name="com.example.zaragozaapps.touchgame.InitTouchGame"
            android:label="@string/touch_game"
            android:parentActivityName="com.example.zaragozaapps.zaragozaapps.MainActivity" >
            <meta-data
                android:name="Touch Game"
                android:value="com.example.zaragozaapps.touchgame.InitTouchGame" />
        </activity>

        <activity
            android:name="com.example.zaragozaapps.zaragozaapps.SetUserName"
            android:label="@string/name_user"
            android:parentActivityName="com.example.zaragozaapps.zaragozaapps.MainActivity" >
            <meta-data
                android:name="Set user name"
                android:value="com.example.zaragozaapps.zaragozaapps.SetUserName" />
        </activity>

        <activity
            android:name="com.example.zaragozaapps.zaragozaapps.Info"
            android:label="@string/info"
            android:parentActivityName="com.example.zaragozaapps.zaragozaapps.MainActivity" >
            <meta-data
                android:name="Information"
                android:value="com.example.zaragozaapps.zaragozaapps.Info" />
        </activity>

        <activity
            android:name="com.example.zaragozaapps.zaragozaapps.Ranking"
            android:label="@string/ranking"
            android:parentActivityName="com.example.zaragozaapps.zaragozaapps.MainActivity" >
            <meta-data
                android:name="Best players"
                android:value="com.example.zaragozaapps.zaragozaapps.Ranking" />
        </activity>

        <activity
            android:name="com.example.zaragozaapps.plusplusgame.PlusPlusActivity"
            android:label="@string/plusplus"
            android:parentActivityName="com.example.zaragozaapps.zaragozaapps.MainActivity" >
            <meta-data
                android:name="Maths brain"
                android:value="com.example.zaragozaapps.plusplusgame.PlusPlusActivity" />
        </activity>

        <activity
            android:name="com.example.zaragozaapps.plusplusgame.PlusPlusInitActivity"
            android:label="@string/plusplus"
            android:parentActivityName="com.example.zaragozaapps.zaragozaapps.MainActivity" >
            <meta-data
                android:name="Maths brain"
                android:value="com.example.zaragozaapps.plusplusgame.PlusPlusInitActivity" />
        </activity>

        <activity
            android:name="com.example.zaragozaapps.matchpaisgame.MatchPairsActivity"
            android:label="@string/matchPairs"
            android:parentActivityName="com.example.zaragozaapps.zaragozaapps.MainActivity" >
            <meta-data
                android:name="Match pairs"
                android:value="com.example.zaragozaapps.matchpaisgame.MatchPairsActivity" />
        </activity>

        <activity
            android:name="com.example.zaragozaapps.matchpaisgame.MatchPairsInitActivity"
            android:label="@string/matchPairs"
            android:parentActivityName="com.example.zaragozaapps.zaragozaapps.MainActivity" >
            <meta-data
                android:name="Match pairs"
                android:value="com.example.zaragozaapps.matchpaisgame.MatchPairsInitActivity" />
        </activity>



    </application>

</manifest>
